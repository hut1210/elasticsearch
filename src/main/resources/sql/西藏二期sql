drop table soms_report.waybill_wide;
CREATE TABLE soms_report.`waybill_wide` (
  `id` varchar(100) NOT NULL COMMENT '主键',
  `busi_name` varchar(100) DEFAULT NULL,
  `require_start_time` datetime DEFAULT NULL,
  `good_weight` varchar(100) DEFAULT NULL,
  `pickup_site_area` varchar(100) DEFAULT NULL,
  `first_time_date` varchar(32) DEFAULT NULL,
  `store_city_name` varchar(100) DEFAULT NULL,
  `store_city_id` varchar(100) DEFAULT NULL,
  `from_county_name` varchar(100) DEFAULT NULL,
  `pickup_site_name` varchar(100) DEFAULT NULL,
  `arrive_area_id` varchar(100) DEFAULT NULL,
  `first_time` datetime DEFAULT NULL,
  `distribute_store_name` varchar(100) DEFAULT NULL,
  `payment` varchar(100) DEFAULT NULL,
  `pickup_site_id` varchar(100) DEFAULT NULL,
  `state` varchar(100) DEFAULT NULL,
  `from_city_id` varchar(100) DEFAULT NULL,
  `real_start_time` datetime DEFAULT NULL,
  `picking_store_name` varchar(100) DEFAULT NULL,
  `from_province_name` varchar(100) DEFAULT NULL,
  `operator_site` varchar(100) DEFAULT NULL,
  `state_create_time` datetime DEFAULT NULL,
  `received_money` varchar(100) DEFAULT NULL,
  `good_volume` varchar(100) DEFAULT NULL,
  `from_province_id` varchar(100) DEFAULT NULL,
  `store_province_id` varchar(100) DEFAULT NULL,
  `store_county_id` varchar(100) DEFAULT NULL,
  `good_number` varchar(100) DEFAULT NULL,
  `picking_store_id` varchar(100) DEFAULT NULL,
  `update_time` datetime DEFAULT NULL,
  `third_waybill_code` varchar(100) DEFAULT NULL,
  `to_county_name` varchar(100) DEFAULT NULL,
  `business_type` varchar(100) DEFAULT NULL,
  `c_id` varchar(100) DEFAULT NULL,
  `from_county_id` varchar(100) DEFAULT NULL,
  `cod_money` varchar(100) DEFAULT NULL,
  `distribute_type` varchar(100) DEFAULT NULL,
  `old_site_id` varchar(100) DEFAULT NULL,
  `to_county_id` varchar(100) DEFAULT NULL,
  `from_city_name` varchar(100) DEFAULT NULL,
  `arrive_area` varchar(100) DEFAULT NULL,
  `old_site_name` varchar(100) DEFAULT NULL,
  `store_province_name` varchar(100) DEFAULT NULL,
  `to_province_name` varchar(100) DEFAULT NULL,
  `old_site_area` varchar(100) DEFAULT NULL,
  `operator_site_id` varchar(100) DEFAULT NULL,
  `to_city_name` varchar(100) DEFAULT NULL,
  `real_start_date` datetime DEFAULT NULL,
  `rel_waybill_code` varchar(100) DEFAULT NULL,
  `to_city_id` varchar(100) DEFAULT NULL,
  `store_county_name` varchar(100) DEFAULT NULL,
  `distribution_type` varchar(100) DEFAULT NULL,
  `to_province_id` varchar(100) DEFAULT NULL,
  `distribute_store_id` varchar(100) DEFAULT NULL,
  `ts` timestamp(3) NOT NULL DEFAULT CURRENT_TIMESTAMP(3) ON UPDATE CURRENT_TIMESTAMP(3) COMMENT '时间戳',
  PRIMARY KEY (`id`),
  KEY `idx_ts` (`ts`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='运单宽表'

drop table soms_report.`waybill_wide_oms`;
CREATE TABLE soms_report.`waybill_wide_oms` (
  `id` varchar(100) NOT NULL COMMENT '主键',
  `wb_no` varchar(200) DEFAULT NULL COMMENT '运单号',
  `source_system` char(1) DEFAULT NULL COMMENT '运单来源',
  `c_id` bigint(20)  DEFAULT null COMMENT 'ID',
  `first_time` datetime DEFAULT NULL COMMENT '创建时间',
  `first_time_date` varchar(32) DEFAULT NULL,
  `good_number` varchar(100) DEFAULT NULL,
  `business_type` char(1) DEFAULT NULL,
  `from_province_id` varchar(100) DEFAULT NULL,
  `from_province_name` varchar(100) DEFAULT NULL,
  `from_city_id` varchar(100) DEFAULT NULL,
  `from_city_name` varchar(100) DEFAULT NULL,
  `from_county_id` varchar(100) DEFAULT NULL,
  `from_county_name` varchar(100) DEFAULT NULL,
  `pickup_site_id` varchar(100) DEFAULT NULL,
  `pickup_site_name` varchar(100) DEFAULT NULL,
  `to_province_id` varchar(100) DEFAULT NULL,
  `to_province_name` varchar(100) DEFAULT NULL,
  `to_city_id` varchar(100) DEFAULT NULL,
  `to_city_name` varchar(100) DEFAULT NULL,
  `old_site_id` varchar(100) DEFAULT NULL,
  `old_site_name` varchar(100) DEFAULT NULL,
  `distribute_store_id` varchar(100) DEFAULT NULL,
  `distribute_store_name` varchar(100) DEFAULT NULL,
  `store_province_id` varchar(100) DEFAULT NULL,
  `store_province_name` varchar(100) DEFAULT NULL,
  `store_city_name` varchar(100) DEFAULT NULL,
  `store_city_id` varchar(100) DEFAULT NULL,
  `state_create_time` datetime DEFAULT NULL,
  `state` varchar(100) DEFAULT NULL,
  `update_time` datetime DEFAULT NULL,
  `operator_site_id` varchar(100) DEFAULT NULL,
  `operator_site` varchar(100) DEFAULT NULL,
  `good_volume` varchar(100) DEFAULT NULL,
  `good_weight` varchar(100) DEFAULT NULL,
  `send_network_id` varchar(100) DEFAULT NULL,
  `send_network_name` varchar(100) DEFAULT NULL,
  `send_network_province_id` varchar(100) DEFAULT NULL,
  `send_network_province_name` varchar(100) DEFAULT NULL,
  `send_network_city_id` varchar(100) DEFAULT NULL,
  `send_network_city_name` varchar(100) DEFAULT NULL,
  `receiver_network_id` varchar(100) DEFAULT NULL,
  `receiver_network_name` varchar(100) DEFAULT NULL,
  `receiver_network_province_id` varchar(100) DEFAULT NULL,
  `receiver_network_province_name` varchar(100) DEFAULT NULL,
  `receiver_network_city_id` varchar(100) DEFAULT NULL,
  `receiver_network_city_name` varchar(100) DEFAULT NULL,
  `site_city_id` varchar(100) DEFAULT NULL,
  `site_city_name` varchar(100) DEFAULT NULL,
  `sorting_city_id` varchar(100) DEFAULT NULL,
  `sorting_city_name` varchar(100) DEFAULT NULL,
  `received_time` varchar(100) DEFAULT NULL,
  `reviewed_time` varchar(100) DEFAULT NULL,
  `inspection_time` varchar(100) DEFAULT NULL,
  `delivery_time` varchar(100) DEFAULT NULL,
  `closing_time` varchar(100) DEFAULT NULL,
  `unsealed_vehicle_time` varchar(100) DEFAULT NULL,
  `cancel_closing_time` varchar(100) DEFAULT NULL,
  `car_assigned_time` varchar(100) DEFAULT NULL,
  `sign_in_time` varchar(100) DEFAULT NULL,
  `picked_up_time` varchar(100) DEFAULT NULL,
  `return_to_sign_in_time` varchar(100) DEFAULT NULL,
  `site_inspection_time` varchar(100) DEFAULT NULL,
  `site_delivery_time` varchar(100) DEFAULT NULL,
  `transport_time` varchar(100) DEFAULT NULL,
  `arrive_time` varchar(100) DEFAULT NULL,
  `site_again_time` varchar(100) DEFAULT NULL,
  `dispatch_time` varchar(100) DEFAULT NULL,
  `delivered_time` varchar(100) DEFAULT NULL,
  `cancel_deliver_time` varchar(100) DEFAULT NULL,
  `intercept_success_time` varchar(100) DEFAULT NULL,
  `closed_order_time` varchar(100) DEFAULT NULL,
  `partial_sign_in_time` varchar(100) DEFAULT NULL,
  `rejection_time` varchar(100) DEFAULT NULL,
  `transfer_time` varchar(100) DEFAULT NULL,
  `sorter_line_time` varchar(100) DEFAULT NULL,
  `transport_line_time` varchar(100) DEFAULT NULL,
  `delivery_line_time` varchar(100) DEFAULT NULL,
  `wayfinish_line_time` varchar(100) DEFAULT NULL,
  `ts` timestamp(3) NOT NULL DEFAULT CURRENT_TIMESTAMP(3) ON UPDATE CURRENT_TIMESTAMP(3) COMMENT '时间戳',
  PRIMARY KEY (`id`),
  KEY `idx_ts` (`ts`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='OMS运单宽表'


drop table soms_report.transport;
CREATE TABLE soms_report.`transport` (
  `id` varchar(100) NOT NULL COMMENT '主键',
  `begin_node_id` varchar(100) DEFAULT NULL,
  `distance` varchar(100) DEFAULT NULL,
  `vehicle_job_type` varchar(100) DEFAULT NULL,
  `transportation_time` varchar(100) DEFAULT NULL,
  `begin_node_name` varchar(100) DEFAULT NULL,
  `end_node_id` varchar(100) DEFAULT NULL,
  `end_city_name` varchar(100) DEFAULT NULL,
  `carrier_type` varchar(100) DEFAULT NULL,
  `end_node_type` varchar(100) DEFAULT NULL,
  `begin_node_type_name` varchar(100) DEFAULT NULL,
  `end_node_name` varchar(100) DEFAULT NULL,
  `begin_node_type` varchar(100) DEFAULT NULL,
  `seal_car_time` varchar(100) DEFAULT NULL,
  `trans_type` varchar(100) DEFAULT NULL,
  `begin_node_code` varchar(100) DEFAULT NULL,
  `trans_way` varchar(100) DEFAULT NULL,
  `create_time` varchar(100) DEFAULT NULL,
  `begin_city_name` varchar(100) DEFAULT NULL,
  `end_city_id` varchar(100) DEFAULT NULL,
  `arrive_car_time` varchar(100) DEFAULT NULL,
  `begin_city_id` varchar(100) DEFAULT NULL,
  `end_node_type_name` varchar(100) DEFAULT NULL,
  `create_time_date` varchar(100) DEFAULT NULL,
  `end_node_code` varchar(100) DEFAULT NULL,
  `update_time` varchar(100) DEFAULT NULL,
  `ts` timestamp(3) NOT NULL DEFAULT CURRENT_TIMESTAMP(3) ON UPDATE CURRENT_TIMESTAMP(3) COMMENT '时间戳',
  PRIMARY KEY (`id`),
  KEY `idx_ts` (`ts`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='运输宽表'

DROP table if exists soms_report.xz_network;
CREATE TABLE soms_report.`xz_network` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键',
  `network_code` varchar(20) DEFAULT NULL COMMENT '网点编号',
  `network_name` varchar(50) DEFAULT NULL COMMENT '网点名称',
  `network_area` varchar(50) DEFAULT NULL COMMENT '网点所属区域',
  `network_business_type` char(1) DEFAULT NULL COMMENT '网点业务类型',
  `detail_address` varchar(100) DEFAULT NULL COMMENT '网点详细地址',
  `latitude` varchar(50) DEFAULT NULL COMMENT '纬度',
  `longitude` varchar(50) DEFAULT NULL COMMENT '经度',
  `create_time` datetime DEFAULT NULL COMMENT '创建时间',
  `update_time` datetime DEFAULT NULL COMMENT '更新时间',
  `create_user` varchar(100) DEFAULT NULL COMMENT '创建人',
  `update_user` varchar(100) DEFAULT NULL COMMENT '更新人',
  `is_delete` tinyint(4) NOT NULL COMMENT '是否删除',
  `ts` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '时间戳',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='西藏网点';

